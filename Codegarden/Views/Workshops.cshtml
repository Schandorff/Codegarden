@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Workshops>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "SiteMaster.cshtml";

    var workShopQuery = Model.Content.Site().Descendants<Workshop>();
}

<div class="row page-banner">
    <div class="center">
        <img src="../media/page-banner.jpg" alt="">
        <div class="overlay">
            <h1>Codegarden 2018 speakers</h1>
        </div>
    </div>
</div>
<div class="row breadcrumbs">
    <div class="container">
        <a href="">HOME</a> // <span class="yellow">SPEAKERS</span>
    </div>
</div>
<div class="row">
    <div class="container noPadding">
        @foreach (var workshop in workShopQuery)
        {
            <div class="workshop">
                <div class="col-md-2">
                    @workshop.Id
                </div>
                <div class="col-md-7">

                </div>
            </div>
        }
        
    </div>
</div>